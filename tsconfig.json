{
	"compilerOptions": {
		"target": "es2021",
		"lib": ["es2021"],
		"module": "commonjs",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"incremental": true
	},
	"exclude": ["node_modules/"],
	"include": ["**/*.ts"]
}
