{
	"compilerOptions": {
		"target": "es2021",
		"lib": ["es2021"],
		"resolveJsonModule": true,
		"incremental": true,
		"module": "commonjs",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"exclude": ["../src/frontend"],
	"include": ["../src/**/*.ts"]
}
